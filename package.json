{
  "name": "form",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "curiousercreative",
  "license": "MIT",
  "repository": "curiousercreative/form",
  "main": "dist/index.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf ./dist/* && babel ./src --out-dir ./dist --ignore './src/**/*.test.js' --ignore './src/**/*.test.jsx' --ignore './src/**/__tests__/' --source-maps --keep-file-extension --copy-files",
    "start": "cd example && yarn install && yarn start",
    "predeploy": "yarn build && cd example && yarn install && yarn build"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "jsdoc-memberof-namespace": "^2.2.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "memoize-one": ">=3.0.0",
    "pubsub-js": "^1.8.0"
  }
}
